{
    "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
    "include": [
      "src/renderer/src/**/*",
      "src/preload/**/*",
      "src/shared/**/*"
    ],
    "compilerOptions": {
      "composite": true,
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@renderer/*": ["src/renderer/src/*"]
      }
    }
}
  